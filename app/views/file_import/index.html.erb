<div class="row text-center">
  <div class="col-md-6 col-md-offset-3">
    <%= render 'shared/active_bank' %>
  </div>
  <div class="col-md-12">
    <h1><%= I18n.t(:file_import) %></h1>
  </div>
</div>
<br>
<div class="row text-center">
  <div class="col-md-4 col-md-offset-4 col-sm-6 col-sm-offset-3">
    <%= bootstrap_form_tag(
        remote: true, url: "/file_import", method: :post, html: { id: 'file-import-form' }) do |f| %>
      <%= f.file_field(:file, hide_label: true, class: 'filestyle', 'data-buttonText' => I18n.t(:file)) %>
      <%= f.primary I18n.t('import') %>
    <% end %>
  </div>
</div>
<br>
